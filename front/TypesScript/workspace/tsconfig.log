{
  "compilerOptions": { 
    "module": "commonjs", 
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "target" :"ES5",
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": ".",
    "sourceMap": true,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "paths": {"*" : ["node_modules/*"] }
  },
  "include": ["src/**/*"] // 대상 파일 목록을 나타냄
}
